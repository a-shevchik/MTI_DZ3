
public class KPP {
	
	public static void main(String[] args) {
		
		PassengerVehicle lada, honda, mercedes;
		CargoVehicle kamaz, man, iveco;
		
		lada = new PassengerVehicle();
		lada.Name = "Лада";
		lada.Speed = 90;
		lada.SpecTransp = true;
		
		honda = new PassengerVehicle();
		honda.Name = "Honda";
		honda.SpecTransp = false;
		honda.Speed = 110;
		
		mercedes = new PassengerVehicle();
		mercedes.Name = "Mercedes";
		mercedes.SpecTransp = false;
		mercedes.Speed = 200;
		
		kamaz = new CargoVehicle();
		kamaz.Name = "Камаз";
		kamaz.Speed = 60;
		kamaz.SpecTransp = true;
		kamaz.Height = 3;
		kamaz.isTrailer = false;
		kamaz.Weight = 5;
		
		man = new CargoVehicle();
		man.Name = "MAN";
		man.Height = 4;
		man.isTrailer = true;
		man.SpecTransp = false;
		man.Speed = 90;
		man.Weight = 15;
		
		iveco = new CargoVehicle();
		iveco.Name = "IVECO";
		iveco.Height = 5;
		iveco.isTrailer = false;
		iveco.SpecTransp = false;
		iveco.Speed = 50;
		iveco.Weight = 15;
		
		try {		
			lada.printcarinfo();//Информация об автомобиле
			System.out.println("Стоимость проезда: " + lada.calculatetarif() + " руб.");
			System.out.println();
		}
		catch (RuntimeException e) {
			System.out.println("Исключение времени выполнения: " + e);
		}
		finally {
		}

		try {			
			honda.printcarinfo();//Информация об автомобиле
			System.out.println("Стоимость проезда: " + honda.calculatetarif() + " руб.");
			System.out.println();
		}
		catch (RuntimeException e) {
			System.out.println("Исключение времени выполнения: " + e);
		}
		finally {
		}

		try {
			mercedes.printcarinfo();//Информация об автомобиле
			System.out.println("Стоимость проезда: " + mercedes.calculatetarif() + " руб.");
			System.out.println();
		}
		catch (RuntimeException e) {
			System.out.println("Исключение времени выполнения: " + e);
		}
		finally {
		}
			
		try {
			kamaz.printcarinfo();//Информация об автомобиле
			System.out.println("Стоимость проезда: " + kamaz.calculatetarif() + " руб.");
			System.out.println();
		}
		catch (RuntimeException e) {
			System.out.println("Исключение времени выполнения: " + e);
		}
		finally {
		}

			
		
	}

}
